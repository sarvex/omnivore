    "lint": "eslint src --ext ts,js,tsx,jsx",
    "compile": "tsc",
    "build": "tsc",
    "start": "functions-framework --target=textToSpeechHandler",
    "start_streaming": "functions-framework --target=textToSpeechStreamingHandler",
